    <div class="prices-block content content-center" id="sign_up">
        <div class="container">
            <h2><strong>@APIInterface.Resources.HomeResource.Register @APIInterface.Resources.HomeResource.User</strong> </h2>
            <div class="row">
                <!-- Pricing item BEGIN -->
                <div class="col-md-12 col-sm-6 col-xs-12">
                    <div class="pricing-item">
                        <div class="pricing-head" id="sign_up_Reg">
                            <h3>@APIInterface.Resources.HomeResource.PleaseFill</h3>
                        </div>
                        <div class="pricing-content">
                            @model APIInterface.Models.RegisterViewModel
                            @{
                                ViewBag.Title = "Register";
                            }
                            @using (Html.BeginForm("Index", "Home", FormMethod.Post, new { id = "registerUserForm", @class = "form-horizontal  custom-group", role = "form" }))
                            {
                                var accountTypes = new List<SelectListItem>()
                                {
                                    new SelectListItem {Text = "Free", Value = "1"},
                                    new SelectListItem {Text = "Pro", Value = "2"},
                                    new SelectListItem {Text = "Ultimate", Value = "3"}
                                };
                                @Html.AntiForgeryToken()
                                @Html.ValidationSummary("", new { @class = "text-danger validation-summary-custom" })
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.Email</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.TextBoxFor(m => m.Email, new { @class = "form-control signUpFonts", id = "emailRegs" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.Password</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.PasswordFor(m => m.Password, new { @class = "form-control", id = "passRegs" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.ReTypePass</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.PasswordFor(m => m.ConfirmPassword, new { @class = "form-control" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.PhoneNumber</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">&nbsp;</label>
                                    <div class="col-md-9">
                                        @Html.TextBoxFor(m => m.PhoneNumber, new { @class = "form-control signUpFonts", id = "phoneNumber" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.CompanyName</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.TextBoxFor(m => m.CompanyName, new { @class = "form-control signUpFonts", id = "cmpNameRegs" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.CompanyShortURL</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-7">
                                        @Html.TextBoxFor(m => m.ShortUrl, new { @class = "form-control signUpFonts", id = "cmpUrl", placeholder = "http://www.TajeerCare.com/yourCompanyURL" })
                                        <label class="control-label validationAsterikLabel signUpFonts" id="mappedUrl"></label>
                                    </div>
                                    <div class="col-md-2">
                                        <label class="control-label validationAsterikLabel" id="urlErrorLabel">Checking availability...</label>
                                    </div>
                                </div>

                                <div class="form-group" id="div_4">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.Country</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.DropDownListFor(dropdown => dropdown.CountryName, new SelectList(Model.CountryList), "--Select Country--", new { @class = "form-control signUpFonts" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.AccountType</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.DropDownListFor(dropdown => dropdown.AccountType, new SelectList(accountTypes, "Value", "Text", 1), "--Select Account Type--", new { @class = "form-control signUpFonts" })
                                    </div>
                                </div>
                                <div class="form-group">
                                    <label class="col-md-2 control-label signUpFonts">@APIInterface.Resources.HomeResource.Address</label>
                                    <label class="col-md-1 control-label validationAsterikLabel">*</label>
                                    <div class="col-md-9">
                                        @Html.TextAreaFor(m => m.CompanyAddress, new { @class = "form-control signUpFonts", id = "addressRegs" })
                                    </div>
                                </div>

                                <div class="form-group">
                                    <label class="col-md-3 control-label validationAsterikLabel">&nbsp;</label>
                                    <div class="pricing-footer col-md-9">
                                        <button type="submit" disabled class="btn btn-default signUpFonts" id="registraionButton">@APIInterface.Resources.HomeResource.Register</button>
                                    </div>
                                </div>
                            }
                        </div>
